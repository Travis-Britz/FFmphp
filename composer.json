{
    "name": "tbritz/ffmphp",
    "description": "Convert media, save previews, and more. All with clean, elegant syntax that won't get in your way.",
    "keywords": [
        "FFmpeg",
        "video",
        "converting",
        "transcoding",
        "thumbnail",
        "preview"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.1.3",
        "symfony/process": "^4.2|^5.0"
    },
    "authors": [
        {
            "name": "Travis Britz",
            "email": "travisbritz@outlook.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "FFmphp\\": "src/FFmphp/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
